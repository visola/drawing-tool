{
    "env": {
        "amd": true,
        "browser": true
    },
    "extends": [
        "google"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "max-len": ["error", { "code": 120 } ],
        "no-restricted-globals": ["error", { "name": "document", "message": "Import the exported document."}],
        "require-jsdoc": "off"
    }
}